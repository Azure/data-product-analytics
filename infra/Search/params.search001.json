{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "{regionName}"
    },
    "searchName": {
      "value": "datalanding-search001"
    },
    "searchSku": {
      "value": "standard"
    },
    "searchReplicaCount": {
      "value": 1
    },
    "searchPartitionCount": {
      "value": 1
    },
    "searchHostingMode": {
      "value": "default"
    },
    "subnetId": {
      "value": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/virtualNetworks/{vnetName}/subnets/{subnetName}"
    },
    "privateDnsZoneId": {
      "value": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/privateDnsZones/privatelink.search.windows.net"
    }
  }
}
