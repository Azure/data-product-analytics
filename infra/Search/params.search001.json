{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "searchName": {
      "value": "mydatalandingzonename-search001"
    },
    "searchSku": {
      "value": "standard"
    },
    "searchReplicaCount": {
      "value": 1
    },
    "searchPartitionCount": {
      "value": 1
    },
    "searchHostingMode": {
      "value": "default"
    },
    "subnetId": {
      "value": "<my-subnet-resource-id>"
    },
    "privateDnsZoneId": {
      "value": "<my-global-dns-resource-group-resource-id>/providers/Microsoft.Network/privateDnsZones/privatelink.search.windows.net"
    }
  }
}
