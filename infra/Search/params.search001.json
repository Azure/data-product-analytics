{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "searchName": {
      "value": "<my-data-landing-zone-name>-search001"
    },
    "searchSku": {
      "value": "standard"
    },
    "searchReplicaCount": {
      "value": 1
    },
    "searchPartitionCount": {
      "value": 1
    },
    "searchHostingMode": {
      "value": "default"
    },
    "subnetId": {
      "value": "<my-subnet-resource-id>"
    },
    "privateDnsZoneId": {
      "value": "/subscriptions/<my-management-subscription-id>/resourceGroups/<my-global-dns-resource-group-name>/providers/Microsoft.Network/privateDnsZones/privatelink.search.windows.net"
    }
  }
}
