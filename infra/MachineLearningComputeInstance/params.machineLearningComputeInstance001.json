{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "mlComputeInstanceName": {
      "value": "ci001"
    },
    "mlWorkspaceId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/<my-data-landing-zone-name>-rg/providers/Microsoft.MachineLearningServices/workspaces/<my-data-landing-zone-name>-machinelearning001"
    },
    "mlComputeInstanceUserObjectId": {
      "value": "<my-ml-compute-instance-user-object-id>"
    },
    "mlComputeInstanceSshKey": {
      "value": "<your-secure-ssh-key>"
    },
    "mlComputeInstanceVmSize": {
      "value": "Standard_DS3_v2"
    },
    "subnetId": {
      "value": "<my-subnet-resource-id>"
    }
  }
}
