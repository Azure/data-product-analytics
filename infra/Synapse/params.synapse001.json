{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "{regionName}"
    },
    "synapseWorkspaceName": {
      "value": "datalandingz-synapse001"
    },
    "synapseDefaultStorageAccountName": {
      "value": "{synapseStorageAccountName}"
    },
    "synapseDefaultStorageAccountFileSystem": {
      "value": "{synapseStorageAccountFileSystemName}"
    },
    "synapseSqlAdministratorLogin": {
      "value": "ServerMainUser"
    },
    "synapseSqlAdministratorPassword": {
      "value": "<your-secure-password>"
    },
    "synapseDatalakeAnalyticsResourceId": {
      "value": ""
    },
    "synapseComputeSubnetResourceId": {
      "value": ""
    },
    "synapseSqlAdminGroupName": {
      "value": ""
    },
    "synapseSqlAdminGroupObjectID": {
      "value": ""
    },
    "purviewId": {
      "value": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Purview/accounts/{purviewName}"
    },
    "subnetId": {
      "value": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/virtualNetworks/{vnetName}/subnets/{subnetName}"
    },
    "privateDnsZoneIdSql": {
      "value": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/privateDnsZones/privatelink.sql.azuresynapse.net"
    },
    "privateDnsZoneIdDev": {
      "value": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/privateDnsZones/privatelink.dev.azuresynapse.net"
    }
  }
}
