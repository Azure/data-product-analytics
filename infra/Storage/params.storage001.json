{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "{regionName}"
    },
    "storageAccountName": {
      "value": "dataproductstorage001"
    },
    "storageAccountType": {
      "value": "Standard_LRS"
    },
    "enableHns": {
      "value": false
    },
    "subnetId": {
      "value": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/virtualNetworks/{vnetName}/subnets/{subnetName}"
    },
    "privateDnsZoneIdBlob": {
      "value": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/privateDnsZones/privatelink.blob.core.windows.net"
    },
    "privateDnsZoneIdFile": {
      "value": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/privateDnsZones/privatelink.file.core.windows.net"
    }
  }
}
